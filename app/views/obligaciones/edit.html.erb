<div class="row mb-3">
  <div class="offset-md-1 col-md-3">
    <h1>Editar obligacion</h1>
  </div>
</div>
<%= render "form", obligacion: @obligacion, cliente: @cliente %>

<div class="row mb-3">
  <div class="offset-md-1 col-md-1">
    <%= button_to "Eliminar", cliente_obligacion_path(@cliente,@obligacion), {:method => :delete, :class => 'btn btn-danger'} %>
  </div>
</div>
<div class="row mb-3">
  <div class="offset-md-1 col-md-3">
    <%= link_to "Regresar a todas las obligaciones", cliente_obligaciones_path %>
  </div>
</div>

